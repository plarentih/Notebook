<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.enterprise.lu.uni.notebook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.enterprise.lu.uni.notebook.app.orm.MyApplication">
        <activity
            android:name=".app.activity.NotebookActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".app.activity.AddWordActivity"></activity>
        <activity android:name=".app.activity.StartingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".app.activity.WordListActivity"></activity>

        <meta-data
            android:name="AA_MODELS"
            android:value="com.enterprise.lu.uni.notebook.Activities.Model.NewWord, com.enterprise.lu.uni.notebook.Activities.Model.Domain" />
        <meta-data android:name="AA_DB_NAME" android:value="Notebook.db" />
        <meta-data android:name="AA_DB_VERSION" android:value="3" />
    </application>

</manifest>